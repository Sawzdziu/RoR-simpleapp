<div class="hero-unit">
  <h1>Nowy Artykuł</h1>

  <%= form_for(:artykul, :url => {:action => 'utworz', :strona_id => @stronaID.id}) do |pole|%>

      <%= render(:partial => "form", :locals => {:pole => pole})%>

      <div style = "text-align: center">
        <%= link_to("Powrót", {:action => 'index', :strona_id => @stronaID.id}, :class =>'btn btn-warning')%>
        <%= submit_tag("Utwórz", :class => 'btn btn-primary')%>
      </div>

  <% end %>
</div>